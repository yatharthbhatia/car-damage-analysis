{
  "version": 2,
  "builds": [
    {
      "src": "yolo/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": ["yolo/app.py", "yolo/static/**", "yolo/templates/**", "yolo/yolov5s.pt"],
        "excludeFiles": ["yolo/runs/**", "yolo/data/**", "yolo/classify/**", "yolo/segment/**", "dataset/**", "models/**", "docs/**", "src/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "yolo/app.py"
    }
  ]
}